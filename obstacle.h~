#include <vector>
#include <iostream>

class Point;

using namespace std;

class Obstacle{

  public:
    Obstacle(){};
    Obstacle(vector<Point*> vertices){
      vertices_ = vertices;
    };
    ~Obstacle();
    int get_num_vert();
    Point* next_vertice(Point*);
    Point* previous_vertice(Point*);
    
    
  private:
    vector<Point*> vertices_;
};
